docker run --name=hellosb --rm --log-driver syslog \
 --log-opt syslog-address=udp://172.30.0.10:514 --log-opt tag="hellosb-{{.ID}}" devj2019/hellosb
docker network create --subnet=172.30.0.0/16 customnetwork

docker run -it --rm  --network customnetwork bitnami/kafka:latest \
  kafka-console-consumer.sh --bootstrap-server kafka-server:9092 --topic syslog


docker run -d --name elasticsearch --net customnetwork --hostname elastic -p 9200:9200 -p 9300:9300 -e "discovery.type=single-node" elasticsearch:7.5.2

